{
  "expo": {
    "name": "gettruckingdriver",
    "slug": "gettruckingdriver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/logo-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#424fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kingpawan.gettruckingdriver",
      "buildNumber":"3.1.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "NOTIFICATIONS",
        "CAMERA"
      ],
      "package": "com.kingpawan.gettruckingdriver",
      "versionCode": 7
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
